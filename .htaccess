Options +FollowSymlinks
RewriteEngine on
RewriteRule ^$   ./Public/index.php [L]
RewriteRule ^([a-z]*.[a-z]*)$   ./Public/index.php?page=$1 [L]
RewriteRule ^([a-z]*.[a-z]*.[a-z]*)$   ./Public/index.php?page=$1 [L]
RewriteRule ^([a-z]*.[a-z]*)/([0-9]*)$   ./Public/index.php?page=$1&p=$2 [L]
RewriteRule ^([a-z]*.[a-z]*)/([a-z0-9A-Z]*)$   ./Public/index.php?page=$1&word=$2 [L]
RewriteRule ^([a-z]*.[a-z]*)/([a-z]*)/([0-9]*)$   ./Public/index.php?page=$1&word=$2&p=$3 [L]
